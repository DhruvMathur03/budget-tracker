{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": ["error", { "code": 120 }],
        "semi": ["error", "always"]
    }
}

